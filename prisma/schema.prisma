generator client {
  provider        = "prisma-client-js"
  binaryTargets   = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
}


model ResumeSection {
  id        Int      @id @default(autoincrement())
  resumeId String
  section  String
  content  String
  
  @@map("resume_sections")
}